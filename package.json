{
  "name": "cra-ssr",
  "homepage": "https://cra-ssr.herokuapp.com",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "8.10.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "classnames": "^2.2.6",
    "connected-react-router": "^4.3.0",
    "cookie-parser": "^1.4.3",
    "forcedomain": "^1.0.0",
    "hellosign-embedded": "^1.6.1",
    "ignore-styles": "^5.0.1",
    "isomorphic-cookie": "^1.2.4",
    "js-cookie": "^2.2.0",
    "md5-file": "^4.0.0",
    "moment": "^2.22.2",
    "morgan": "^1.9.0",
    "node-sass-chokidar": "^1.3.3",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.13.4",
    "react": "^16.4.0",
    "react-currency-format": "^1.0.0",
    "react-dom": "^16.4.0",
    "react-frontload": "^1.0.1",
    "react-helmet": "^5.2.0",
    "react-loadable": "^5.4.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-scripts": "1.1.4",
    "reactstrap": "^6.4.0",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --write",
      "git add"
    ]
  },
  "scripts": {
    "build-css": "node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar --include-path ./src --include-path ./node_modules src/ -o src/ --watch --recursive --source-map true",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged",
    "serve": "NODE_ENV=production node ./server/index.js"
  }
}
